/**
 * TeamLeadDashboard - Crisis Services Team Management System
 * Entry point, menu creation, and navigation with emojis
 */

function onOpen() {
  var ui = SpreadsheetApp.getUi();
  ui.createMenu('ğŸ“Š Team Lead Dashboard')
    .addItem('ğŸ  Open Dashboard', 'openDashboard')
    .addSeparator()
    .addSubMenu(ui.createMenu('ğŸ‘¥ One-on-One')
      .addItem('ğŸ“ Create Session', 'createOneOnOneSession')
      .addItem('ğŸ‘ï¸ View Sessions', 'viewOneOnOneSessions')
      .addItem('ğŸ“¤ Export Notes', 'exportOneOnOneNotes'))
    .addSubMenu(ui.createMenu('ğŸ‘ª Team Management')
      .addItem('â• Add Team Member', 'addTeamMember')
      .addItem('ğŸ‘¥ View Team', 'viewTeam')
      .addItem('ğŸ”„ Update Status', 'updateTeamMemberStatus'))
    .addSubMenu(ui.createMenu('ğŸ‘¤ Manager One-on-One')
      .addItem('ğŸ“ Create Session', 'createManagerOneOnOneForm')
      .addItem('ğŸ‘ï¸ View Notes', 'viewManagerOneOnOneNotes')
      .addItem('ğŸ“Š Dashboard', 'viewManagerOneOnOneDashboard'))
    .addSubMenu(ui.createMenu('ğŸ“ General Notes')
      .addItem('ğŸ“ Create Note', 'createGeneralNote')
      .addItem('ğŸ‘ï¸ View Notes', 'viewGeneralNotes')
      .addItem('ğŸ“Š Dashboard', 'viewGeneralNotesDashboard'))
    .addSubMenu(ui.createMenu('ğŸ“ˆ Metrics')
      .addItem('ğŸ“ Enter Data', 'enterMetricsData')
      .addItem('ğŸ“Š View Reports', 'viewMetricsReports')
      .addItem('ğŸ“ˆ Dashboard', 'viewMetricsDashboard')
      .addItem('ğŸ“¤ Export Data', 'exportMetricsData')
      .addItem('ğŸ“§ Email Config', 'configureMetricsEmail'))
    .addSubMenu(ui.createMenu('ğŸ” QA Reports')
      .addItem('âœ… Create Review', 'createQAReview')
      .addItem('ğŸ“‹ View Reports', 'viewQAReports')
      .addItem('ğŸ“Š Dashboard', 'viewQADashboard')
      .addItem('ğŸ“¤ Export Data', 'exportQAData'))
    .addSubMenu(ui.createMenu('â±ï¸ Time & Schedule')
      .addItem('â±ï¸ Time Tracker', 'openTimeTracker')
      .addItem('ğŸ“Š Time Reports', 'viewTimeReports')
      .addItem('ğŸ“… Manage Schedule', 'manageSchedule')
      .addItem('ğŸ”„ Initialize Week', 'initializeWeek')
      .addItem('ğŸ“‹ Schedule Reports', 'viewScheduleReports'))
    .addSubMenu(ui.createMenu('âœ… Tasks')
      .addItem('ğŸ“ Create Task', 'createTask')
      .addItem('ğŸ”— Asana Task', 'createAsanaTask')
      .addItem('ğŸ“‹ View Tasks', 'viewTasks'))
    .addSubMenu(ui.createMenu('ğŸ“Œ Action Items')
      .addItem('ğŸ“ Manage Items', 'manageActionItems')
      .addItem('ğŸ‘ï¸ View Items', 'viewActionItems')
      .addItem('ğŸ”” Set Reminders', 'setActionItemReminders'))
    .addSubMenu(ui.createMenu('âš™ï¸ Settings')
      .addItem('ğŸ”§ System Settings', 'openSystemSettings')
      .addItem('â“ Help', 'openHelp')
      .addItem('ğŸ“Š Error Log Viewer', 'openErrorLogViewer')
      .addItem('ğŸ‘¤ User Preferences', 'openUserPreferences')
      .addItem('ğŸ Debug Tool', 'openDebugTool')) // Added debug tool menu item
    .addToUi();
}

/**
 * Opens the dashboard HTML interface
 */
function openDashboard() {
  var html = HtmlService.createTemplateFromFile('HTML/Dashboard/dashboard')
    .evaluate()
    .setWidth(1200)
    .setHeight(800)
    .setTitle('ğŸ“Š Team Lead Dashboard');
  
  SpreadsheetApp.getUi().showModalDialog(html, 'ğŸ“Š Team Lead Dashboard');
}

/**
 * Include HTML templates within other HTML files
 * @param {string} filename - Name of the HTML file to include
 * @return {string} - Content of the HTML file
 */
function include(filename) {
  return HtmlService.createHtmlOutputFromFile(filename).getContent();
}

/**
 * Gets the active user's email address
 * No hard-coding of user information
 * @return {string} - User's email
 */
function getUserEmail() {
  return Session.getActiveUser().getEmail();
}

/**
 * One-on-One Session functions
 */
function createOneOnOneSession() {
  // Clear any cached session ID
  CacheService.getUserCache().remove('currentOneOnOneSession');
  
  var html = HtmlService.createTemplateFromFile('HTML/OneOnOne/one-on-one-form')
    .evaluate()
    .setWidth(900)
    .setHeight(700)
    .setTitle('ğŸ“ Create One-on-One Session');
  
  SpreadsheetApp.getUi().showModalDialog(html, 'ğŸ“ Create One-on-One Session');
}

function viewOneOnOneSessions() {
  var html = HtmlService.createTemplateFromFile('HTML/OneOnOne/one-on-one-list')
    .evaluate()
    .setWidth(1000)
    .setHeight(700)
    .setTitle('ğŸ‘ï¸ View One-on-One Sessions');
  
  SpreadsheetApp.getUi().showModalDialog(html, 'ğŸ‘ï¸ View One-on-One Sessions');
}

