/**
 * Team Lead Dashboard
 * 
 * Main code file containing initialization functions, menu creation,
 * and show functions for all dashboard components.
 */

// Constants for sheet names
const SHEET_NAMES = {
  TEAM_MEMBERS: 'Team Members',
  ONE_ON_ONES: 'One-on-One Sessions',
  MANAGER_ONE_ON_ONES: 'Manager One-on-One Sessions',
  QA_REVIEWS: 'QA Reviews',
  METRICS: 'Metrics',
  ACTION_ITEMS: 'Action Items',
  TASKS: 'Tasks',
  ASANA_TASKS: 'Asana Tasks',
  TIME_TRACKING: 'Time Tracking',
  SCHEDULE: 'Schedule',
  GENERAL_NOTES: 'General Notes',
  CONFIG: 'Config'
};

/**
 * Runs when the spreadsheet is opened
 */
function onOpen() {
  const ui = SpreadsheetApp.getUi();
  
  // Add a custom menu
  ui.createMenu('üìä Dashboard')
    .addItem('üè† View Dashboard', 'showDashboard')
    .addSeparator()
    .addSubMenu(ui.createMenu('üë• Team Management')
      .addItem('üë§ View Team Members', 'showTeamView')
      .addItem('‚ûï Add Team Member', 'showAddTeamMemberForm')
      .addItem('üîÑ Update Status', 'showUpdateStatusForm'))
    .addSeparator()
    .addSubMenu(ui.createMenu('üìã One-on-One Notes')
      .addItem('üìù New Session', 'showOneOnOneForm')
      .addItem('üìä View Sessions', 'showOneOnOneNotes')
      .addItem('üìà Dashboard', 'showOneOnOneDashboard')
      .addItem('üì§ Export Options', 'showOneOnOneExport'))
    .addSeparator()
    .addSubMenu(ui.createMenu('ü§ù Manager One-on-One')
      .addItem('üìù New Session', 'showManagerOneOnOneForm')
      .addItem('üìä View Sessions', 'showManagerOneOnOneNotes')
      .addItem('üìà Dashboard', 'showManagerOneOnOneDashboard'))
    .addSeparator()
    .addSubMenu(ui.createMenu('üìä Metrics')
      .addItem('üìä Daily Metrics', 'showMetricsForm')
      .addItem('üìà Reports', 'showMetricsReports')
      .addItem('üìâ Dashboard', 'showMetricsDashboard')
      .addItem('üìß Email Config', 'showMetricsEmailConfig')
      .addItem('üì§ Export', 'showMetricsExport'))
    .addSeparator()
    .addSubMenu(ui.createMenu('üß™ QA Reviews')
      .addItem('üìù New Review', 'showQAReviewForm')
      .addItem('üìä View Reviews', 'showQAReports')
      .addItem('üìà Dashboard', 'showQADashboard')
      .addItem('üì§ Export', 'showQAExport'))
    .addSeparator()
    .addSubMenu(ui.createMenu('üìù General Notes')
      .addItem('üìù New Note', 'showGeneralNoteForm')
      .addItem('üìä View Notes', 'showGeneralNotes')
      .addItem('üìà Dashboard', 'showGeneralNotesDashboard'))
    .addSeparator()
    .addSubMenu(ui.createMenu('‚è±Ô∏è Time & Schedule')
      .addItem('‚è±Ô∏è Time Tracker', 'showTeamLeadTimeTracker')
      .addItem('üìä Time Reports', 'showTimeReports')
      .addItem('üìÖ Manage Schedule', 'showScheduleManager')
      .addItem('üîÑ Initialize Week', 'showInitializeWeekForm')
      .addItem('üìà Schedule Reports', 'showScheduleReports'))
    .addSeparator()
    .addSubMenu(ui.createMenu('‚úÖ Tasks')
      .addItem('‚úÖ Add Task', 'showTaskForm')
      .addItem('üåê Add Asana Task', 'showAsanaTaskForm')
      .addItem('üìã View Tasks', 'showTasksList'))
    .addSeparator()
    .addSubMenu(ui.createMenu('üìå Action Items')
      .addItem('üìå Add Action Item', 'showActionItemsForm')
      .addItem('üìä View Action Items', 'showActionItemsView')
      .addItem('üîî Reminders', 'showActionItemsReminders'))
    .addSeparator()
    .addSubMenu(ui.createMenu('‚öôÔ∏è Settings & Support')
      .addItem('‚öôÔ∏è System Settings', 'showSettings')
      .addItem('üë§ User Preferences', 'showUserPreferences')
      .addItem('‚ùì Help & Support', 'showHelp'))
    .addToUi();
  
  // Check if the system is initialized
  if (!isSystemInitialized()) {
    initializeSpreadsheet();
  }
}

/**
 * Check if system is initialized
 */
function isSystemInitialized() {
  try {
    const ss = SpreadsheetApp.getActiveSpreadsheet();
    
    // Check for required sheets
    for (const key in SHEET_NAMES) {
      if (!ss.getSheetByName(SHEET_NAMES[key])) {
        return false;
      }
    }
    
    return true;
  } catch (error) {
    console.error(`Error checking if system is initialized: ${error}`);
    return false;
  }
}

/**
 * Initialize the spreadsheet structure with all required sheets
 */
function initializeSpreadsheet() {
  try {
    const ss = SpreadsheetApp.getActiveSpreadsheet();
    const ui = SpreadsheetApp.getUi();
    
    // Show initialization progress
    ui.alert('üöÄ Dashboard Initialization', 
             'Setting up your Team Lead Dashboard. This will create all necessary sheets and configurations.', 
             ui.ButtonSet.OK);
    
    // Initialize each sheet with default headers
    initializeTeamMembersSheet(ss);
    initializeOneOnOnesSheet(ss);
    initializeManagerOneOnOnesSheet(ss);
    initializeQAReviewsSheet(ss);
    initializeMetricsSheet(ss);
    initializeActionItemsSheet(ss);
    initializeTasksSheet(ss);
    initializeAsanaTasksSheet(ss);
    initializeTimeTrackingSheet(ss);
    initializeScheduleSheet(ss);
    initializeGeneralNotesSheet(ss);
    initializeConfigSheet(ss);
    
    // Set the current user as default team lead
    const currentUser = getCurrentUserEmail();
    const userName = getCurrentUserName();
    
    // Update configuration settings
    updateConfigValue('TeamLeadEmail', currentUser);
    updateConfigValue('TeamLeadName', userName);
    updateConfigValue('LastInitialized', new Date().toISOString());
    
    ui.alert('‚úÖ Setup Complete', 
             'Your Team Lead Dashboard has been successfully set up! You can now start using it to manage your team.', 
             ui.ButtonSet.OK);
    
    return true;
  } catch (error) {
    console.error('Error initializing spreadsheet: ' + error);
    SpreadsheetApp.getUi().alert('‚ùå Setup Error', 
                                'There was an error setting up the dashboard: ' + error, 
                                SpreadsheetApp.getUi().ButtonSet.OK);
    return false;
  }
}

// Show functions for all dashboard pages

/**
 * Show the main dashboard
 */
function showDashboard() {
  const html = HtmlService.createTemplateFromFile('dashboard')
    .evaluate()
    .setTitle('Team Lead Dashboard')
    .setWidth(1200)
    .setHeight(900);
  
  SpreadsheetApp.getUi().showModalDialog(html, 'üìä Team Lead Dashboard');
}

/**
 * Show one-on-one form
 */
function showOneOnOneForm() {
  const html = HtmlService.createTemplateFromFile('one-on-one-form')
    .evaluate()
    .setTitle('One-on-One Session')
    .setWidth(900)
    .setHeight(700);
  
  SpreadsheetApp.getUi().showModalDialog(html, 'üìù One-on-One Session');
}

